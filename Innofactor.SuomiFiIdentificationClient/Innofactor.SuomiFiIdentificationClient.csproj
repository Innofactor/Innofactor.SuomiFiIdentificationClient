<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
	<Authors>Innofactor</Authors>
    <Company>Innofactor</Company>
    <Description>Suomi.fi authentication client for dealing with all that SAML2 and decryption</Description>
    <PackageLicenseUrl>https://github.com/Innofactor/Innofactor.SuomiFiIdentificationClient/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Innofactor/Innofactor.SuomiFiIdentificationClient</PackageProjectUrl>
    <PackageIconUrl>http://rappen.net/Cinteros/innofactor.png</PackageIconUrl>
    <RepositoryUrl>>https://github.com/Innofactor/Innofactor.SuomiFiIdentificationClient</RepositoryUrl>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>signkey.snk</AssemblyOriginatorKeyFile>
    <DelaySign>false</DelaySign>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="3.1.1" />
    <PackageReference Include="Sustainsys.Saml2.AspNetCore2" Version="2.8.0" />
  </ItemGroup>

</Project>
